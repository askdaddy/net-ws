{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "class-name": true,
    "max-classes-per-file": false,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-bitwise": false,
    "no-var-requires": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ]
  },
  "rulesDirectory": []
}